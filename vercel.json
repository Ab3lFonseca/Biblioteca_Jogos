{
  "builds": [
    {
      "src": "/pages/**",
      "use": "@vercel/static"
    },
    {
      "src": "/styles/**",
      "use": "@vercel/static"
    },
    {
      "src": "/styles/**",
      "use": "@vercel/static"
    },
    {
      "src": "/itensEsternos/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "src/pages/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "src/pages/$1"
    }
  ]
}